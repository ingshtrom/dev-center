                <!--</tr>-->
                <!--<tr>-->
                <!--<td>Jonathan</td>-->
                <!--<td>Lollipop</td>-->
                <!--</tr>-->
              </tbody>
            </table>
          </div>
        </li>
      {{/dc-collapsible}}
    </div>
  </div>
</div>
